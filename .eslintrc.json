{
  "parser": "babel-eslint",
  "extends": ["airbnb-base/legacy", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "strict": "off",
    "prettier/prettier": "error",
    "global-require": "off",
    "no-case-declarations": "off",
    "no-template-curly-in-string": "off",
    "func-names": "off",
    "no-console": "off",
    "no-param-reassign": "off"
  }
}
